{
  "projectId": "311aceb2-afda-4131-ba5b-17c75b918514",
  "cli": {
    "version": ">= 3.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "latest",
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      },
      "ios": {
        "image": "latest",

        "buildConfiguration": "Debug",
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "image": "latest",

        "buildType": "apk"
      },
      "ios": {
        "image": "latest",
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "ios": {
        "image": "latest",
        "resourceClass": "m-medium"
      },
      "android": {
        "image": "latest",
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
